      <h2><center>PRONTUÁRIO DO PACIENTE</center></h2></p>
      <table pwp:ask="findall([ID,Nome,DataEntrada,DataSaida,HoraEntrada,HoraSaida,HistoricoSaude,PrescMedica,EvoluMedica,PrescEnfermagem,EvoluEnfermagem,Anotacoes,ResultadoExame,ResultadoExameIm,PrescNutri,EvoluNutri,Divergencias], prontuario(ID,Nome,DataEntrada,DataSaida,HoraEntrada,HoraSaida,HistoricoSaude,PrescMedica,EvoluMedica,PrescEnfermagem,EvoluEnfermagem,Anotacoes,ResultadoExame,ResultadoExameIm,PrescNutri,EvoluNutri,Divergencias), Lista1)" width="866" border="1" align="center">
        <tr pwp:ask="member([ID,Nome,DataEntrada,DataSaida,HoraEntrada,HoraSaida,HistoricoSaude,PrescMedica,EvoluMedica,PrescEnfermagem,EvoluEnfermagem,Anotacoes,ResultadoExame,ResultadoExameIm,PrescNutri,EvoluNutri,Divergencias], Lista1)">
          <td width="175" class="cabecalho">Nome do Paciente</td>
          <td width="675" pwp:use="Nome" />
        </tr>
        <tr pwp:ask="member([ID,Nome,DataEntrada,DataSaida,HoraEntrada,HoraSaida,HistoricoSaude,PrescMedica,EvoluMedica,PrescEnfermagem,EvoluEnfermagem,Anotacoes,ResultadoExame,ResultadoExameIm,PrescNutri,EvoluNutri,Divergencias], Lista1)">
          <td width="127" class="cabecalho">Data de Entrada</td>
          <td pwp:use="DataEntrada" />
        </tr>
        <tr pwp:ask="member([ID,Nome,DataEntrada,DataSaida,HoraEntrada,HoraSaida,HistoricoSaude,PrescMedica,EvoluMedica,PrescEnfermagem,EvoluEnfermagem,Anotacoes,ResultadoExame,ResultadoExameIm,PrescNutri,EvoluNutri,Divergencias], Lista1)">
          <td width="127" class="cabecalho">Hora de Entrada</td>
          <td pwp:use="HoraEntrada" />
        </tr>
        <tr pwp:ask="member([ID,Nome,DataEntrada,DataSaida,HoraEntrada,HoraSaida,HistoricoSaude,PrescMedica,EvoluMedica,PrescEnfermagem,EvoluEnfermagem,Anotacoes,ResultadoExame,ResultadoExameIm,PrescNutri,EvoluNutri,Divergencias], Lista1)">
          <td width="127" class="cabecalho">Data de Saída</td>
          <td pwp:use="DataSaida" />
        </tr>
        <tr pwp:ask="member([ID,Nome,DataEntrada,DataSaida,HoraEntrada,HoraSaida,HistoricoSaude,PrescMedica,EvoluMedica,PrescEnfermagem,EvoluEnfermagem,Anotacoes,ResultadoExame,ResultadoExameIm,PrescNutri,EvoluNutri,Divergencias], Lista1)">
          <td width="127" class="cabecalho">Hora de Saida</td>
          <td pwp:use="HoraSaida" />
        </tr>
      </table>
      <br />
      <table pwp:ask="findall([ID,Nome,DataEntrada,DataSaida,HoraEntrada,HoraSaida,HistoricoSaude,PrescMedica,EvoluMedica,PrescEnfermagem,EvoluEnfermagem,Anotacoes,ResultadoExame,ResultadoExameIm,PrescNutri,EvoluNutri,Divergencias], prontuario(ID,Nome,DataEntrada,DataSaida,HoraEntrada,HoraSaida,HistoricoSaude,PrescMedica,EvoluMedica,PrescEnfermagem,EvoluEnfermagem,Anotacoes,ResultadoExame,ResultadoExameIm,PrescNutri,EvoluNutri,Divergencias), Lista2)" width="867" border="1" align="center">
